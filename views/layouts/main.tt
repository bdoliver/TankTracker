<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>TankTracker</title>
    <meta http-equiv="Content-type" content="text/html; charset=[% settings.charset %]" />
   
    <!-- stash the uri_base so that it's accessible from within scripts: -->
    <script>
        var uri_base = '[% request.uri_base %]';
    </script>

    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/jquery-1.9.1.min.js"></script>
    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/jquery.flot.min.js"></script>
    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/jquery.flot.resize.min.js"></script>
    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/jquery.flot.navigate.min.js"></script>
    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/jquery.flot.time.min.js"></script>
    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/jquery-disable.js"></script>
    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/jquery.hiddenDimensions.js"></script>

    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/strftime-min.js"></script>

    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/jquery-ui-1.10.3.custom.min.js"></script>

    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/jquery.ui.selectmenu.js"></script>
 
   
    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/i18n/grid.locale-en.js" type="text/javascript"></script>
    <script type="text/javascript">
            jQuery.jgrid.no_legacy_api = true;
    </script>
    
    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/jquery.jqGrid.src.js" type="text/javascript"></script>

    <script language="javascript" type="text/javascript" src="[% request.uri_base %]/javascripts/TankTracker.js"></script>

    <link rel="StyleSheet" type="text/css" href="[% request.uri_base %]/css/themes/[% jq_theme %]/jquery-ui-1.10.3.custom.min.css" title="none" />
    <link rel="StyleSheet" type="text/css" href="[% request.uri_base %]/css/plot.css" title="none" />
    <link rel="stylesheet" type="text/css" href="[% request.uri_base %]/css/ui.jqgrid.css" title="none" media="screen" />
    <link rel="stylesheet" type="text/css" href="[% request.uri_base %]/css/jquery.ui.selectmenu.css" title="none" media="screen" />

    <link rel="stylesheet" type="text/css" href="[% request.uri_base %]/css/searchFilter.css" title="none" media="screen" />
    <!-- <link rel="stylesheet" type="text/css" href="[% request.uri_base %]/css/ajaxfileupload.css" title="none" media="screen" /> -->

    <link rel="StyleSheet" type="text/css" href="[% request.uri_base %]/css/style.css" title="none" media="screen"/>
</head>
<body>
    <!-- stash the uri_base so that it's accessible to ajax calls: -->
    <script>
        var uri_base = '[% request.uri_base %]';
    </script>

    <div class="content">
        <h1>TankTracker</h1>

        [% IF msg %]<div id="msgText" class="msg">[% msg %]</div>[% END %]
        [% IF err %]<div id="errText" class="err">[% err %]</div>[% END %]

        [% content %]

    </div>

    [% IF session.logged_in %]
    <p>
        <span class="jsLink"><a href="[% request.uri_base %]/logout">Log out</a></span>
    </p>
    [% END %]

    <div id="footer">
        Powered by <a href="http://perldancer.org/">Dancer</a> [% dancer_version %]
        <!-- <img src="/images/perldancer.jpg" /> -->
    </div>
</body>
</html>
